name: "Proper place nouns inner"
description: "Generates a paradigm for proper nouns."
morphology:
  pos: N
lexicon:
  XPATH:
    sem_type: ".//l/@sem_type"
    type: ".//l/@type"
  type: Prop
  sem_type: 
    - "Plc"
    - "plc"
--
{{ lemma }}+N+Prop+Sem/Plc+Sg+Nom
{{ lemma }}+N+Prop+Sem/Plc+Sg+Gen
{{ lemma }}+N+Prop+Sem/Plc+Sg+Par
{{ lemma }}+N+Prop+Sem/Plc+Sg+Ill
{{ lemma }}+N+Prop+Sem/Plc+Sg+Ine
{{ lemma }}+N+Prop+Sem/Plc+Sg+Ela
